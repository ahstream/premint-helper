{
  "name": "premint-helper",
  "version": "1.0.0",
  "description": "Premint Helper",
  "license": "COPYRIGHT",
  "main": "dist/index.js",
  "browserslist": "> 0.25%, not dead",
  "browser": {
    "child_process": false
  },
  "scripts": {
    "dev": "npm run build:dev",
    "prod": "npm run build:prod",
    "build:dev": "webpack --watch --config webpack/webpack.dev.js",
    "build:prod": "npm run patch:manifest && webpack --config webpack/webpack.prod.js",
    "build:publish": "",
    "patch:manifest": "node tools/patchManifest.js",
    "clean": "rm -rf node_modules",
    "clean:install": "npm run clean && npm install",
    "test": "jest",
    "lint": "eslint --ext .js --max-warnings=0 .",
    "prettier": "prettier --write",
    "wip": "git add . && git commit -m \"wip\"",
    "null": ""
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.22.9",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@types/chrome": "^0.0.193",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-webpack-plugin": "^3.2.0",
    "json-bump": "^1.0.2",
    "mini-css-extract-plugin": "^2.6.1",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.6",
    "@types/node": "^20.6.2",
    "crypto-js": "^4.1.1",
    "hx-chrome-lib": "file:../hx-chrome-lib",
    "hx-lib": "file:../hx-lib",
    "hx-statusbar": "file:../hx-statusbar"
  }
}
