{
    "name": "Premint Helper",
    "description": "Premint.xyz and Alphabot.app raffle helpers",
    "version": "1.1.0",
    "manifest_version": 3,
    "key": "",
    "background": {
        "service_worker": "serviceWorker.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://twitter.com/intent/*"
            ],
            "exclude_matches": [],
            "js": [
                "twitterIntentPage.js"
            ],
            "css": [],
            "run_at": "document_start"
        },
        {
            "matches": [
                "https://twitter.com/*"
            ],
            "exclude_matches": [
                "https://twitter.com/intent/*"
            ],
            "js": [
                "twitterPage.js"
            ],
            "css": [],
            "run_at": "document_start"
        },
        {
            "matches": [
                "https://discord.gg/*",
                "https://discord.com/invite/*",
                "https://discord.com/app/*",
                "https://discord.com/channels/*"
            ],
            "exclude_matches": [],
            "css": [],
            "js": [
                "discordPage.js"
            ],
            "run_at": "document_start"
        },
        {
            "matches": [
                "https://www.alphabot.app/",
                "https://www.alphabot.app/?*"
            ],
            "exclude_matches": [],
            "css": [
                "styles/alphabotMainPage.css"
            ],
            "js": [
                "alphabotMainPage.js"
            ],
            "run_at": "document_start"
        },
        {
            "matches": [
                "https://www.alphabot.app/*"
            ],
            "exclude_matches": [
                "https://www.alphabot.app/",
                "https://www.alphabot.app/?*"
            ],
            "css": [
                "styles/alphabotRafflePage.css"
            ],
            "js": [
                "alphabotRafflePage.js"
            ],
            "run_at": "document_start"
        },
        {
            "matches": [
                "https://www.premint.xyz/*"
            ],
            "exclude_matches": [
                "https://www.premint.xyz/collectors/entries/*"
            ],
            "css": [
                "styles/premintPage.css"
            ],
            "js": [
                "premintPage.js"
            ],
            "run_at": "document_start"
        }
    ],
    "permissions": [
        "storage",
        "unlimitedStorage",
        "activeTab",
        "tabs",
        "webNavigation"
    ],
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/icons/16x.png",
            "32": "/icons/32x.png",
            "48": "/icons/48x.png",
            "128": "/icons/128x.png"
        }
    },
    "icons": {
        "16": "/icons/16x.png",
        "32": "/icons/32x.png",
        "48": "/icons/48x.png",
        "128": "/icons/128x.png"
    }
}
